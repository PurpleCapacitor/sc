{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
        "quotemark": [
          false
        ],
        "prefer-const": false,
        "forin": false,
        "whitespace": false,
        "no-trailing-whitespace": false,
        "comment-format": [false, "check-space"]

    }
}
